{
    "autoUpdate": true,
    "autoUpdateNotification": "true",
    "enabledThemes": [
        "Everforest.css",
        "deshitiffy.css"
    ],
    "plugins": {
        "CommandsAPI": {
            "enabled": true
        },
        "MessageAccessoriesAPI": {
            "enabled": true
        },
        "MessageEventsAPI": {
            "enabled": true
        },
        "UserSettingsAPI": {
            "enabled": true
        },
        "FakeNitro": {
            "enabled": "true"
        },
        "CrashHandler": {
            "enabled": true
        },
        "VolumeBooster": {
            "enabled": true
        },
        "BadgeAPI": {
            "enabled": true
        },
        "NoTrack": {
            "enabled": true,
            "disableAnalytics": true
        },
        "Settings": {
            "enabled": true,
            "settingsLocation": "aboveNitro"
        },
        "SupportHelper": {
            "enabled": true
        }
    },
    "notifications": {
        "timeout": 5000,
        "position": "bottom-right",
        "useNative": "not-focused",
        "logLimit": 50
    }
}